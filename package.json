{
  "name": "scully-plugins",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "scully": "npx scully --cf scully.scully-plugin-demo.config.ts --ks --scan --no-warning",
    "scully:serve": "npx scully serve --cf scully.scully-plugin-demo.config.ts",
    "prepare": "husky install",
    "release:scully-plugin-toc": "nx run scully-plugin-toc:version && cd scully/plugins/scully-plugin-toc && npm publish --access public",
    "release:scully-plugin-mermaid": "nx run scully-plugin-mermaid:version && cd scully/plugins/scully-plugin-mermaid && npm publish --access public"
  },
  "private": true,
  "dependencies": {
