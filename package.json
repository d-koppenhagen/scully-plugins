    "prepare": "husky install",
    "release:scully-plugin-toc": "nx run scully-plugin-toc:version && cd scully/plugins/scully-plugin-toc && npm publish",
    "release:scully-plugin-mermaid": "nx run scully-plugin-mermaid:version && cd scully/plugins/scully-plugin-mermaid && npm publish"
  },
